[
  {
    "tag": {
      "pageName": "Home",
      "section": "clickable-page"
    },
    "meta": {
      "name": "home"
    },
    "identifier": "/home",
    "type": "page",
    "event": "page_view",
    "rules": {
      "page": {
        "name": "<pageName>:<section>"
      } 
    }
  },
  {
    "tag": {
      "buttonName": "next-step"
    },
    "meta": {
      "location": "/home"
    },
    "parent": "/home",
    "identifier": "home-next-step",
    "type": "button",
    "event": "button_click",
    "rules": {
      "button": {
        "name": "<buttonName>"
      } 
    }
  }
]